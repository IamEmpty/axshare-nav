
mixin list( obj )
  ul.axshare-page-list
    each val in obj
      if val.name
        li.axshare-page-list__item

          if val.url
            a.axshare-page-text.axshare-text-easy(href= val.url)= val.name
          else
            p.axshare-page-link.axshare-text-easy= val.name

          if val.childs
            +list( val.childs )

// Ul {}
if axshare.nav
  .axshare-page-nav
    .icon-menu-wrapper
      i.icon-menu
    +list( axshare.nav )
