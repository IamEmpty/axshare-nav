
if axshare
  mixin list( obj )
    ul.axshare-page-list
      each val in obj
        if val.name
          li.axshare-page-list__item

            if val.url
              a.axshare-page-text.axshare-text-easy(href= val.url)= val.name
            else
              p.axshare-page-link.axshare-text-easy= val.name

            if val.childs
              +list( val.childs )

  //- ul {}
  if axshare.nav
    .axshare-page-nav
      .axshare-page-nav__icon
        i.icon-menu
      +list( axshare.nav )
